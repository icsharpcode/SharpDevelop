<?xml version="1.0"?>
<Template author="Matt Ward" version="1.0">

	<Config
		name="MVC View Content Page"
		icon="VB.File.WebFile"
		category="VB"
		subcategory="ASP.NET MVC"
		defaultname="ViewContentPage${Number}.aspx"
		language="VBNET"/>
	 
	<Description>MVC View Content Page</Description>
	
	<Files>
