<![CDATA[<%@ Page Language="VB" MasterPageFile="~/Views/Shared/Site.Master" Inherits="System.Web.Mvc.ViewPage" %>

<asp:Content ContentPlaceHolderId="Title" runat="server">
</asp:Content>

<asp:Content ContentPlaceHolderId="Main" runat="server">
</asp:Content>
]]>
		</File>
	</Files>
</Template>
